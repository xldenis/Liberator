<p style="float:right"><%= link_to "New Article", new_article_path%></p>
<% @article.each do |article| %>

<p><%= article.title %> <% if can? :update, article%><%=link_to "Edit Article",edit_article_path(article)%><% end %><% if can? :destroy, article %> <%= link_to "Delete",article,:method => :delete,:remote => true%><% end %></p>

<% end %>
